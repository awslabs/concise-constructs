{
  "$schema": "https://json.schemastore.org/package",
  "name": "concise-constructs",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "main": "cjs/index.js",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs"
  ],
  "description": "A functional-feeling DX for the AWS CDK and other construct libraries",
  "repository": "github:awslabs/concise-constructs",
  "keywords": [
    "concise",
    "constructs",
    "aws",
    "cdk",
    "cdk8s",
    "terraform",
    "cloud",
    "development",
    "kit"
  ],
  "dependencies": {
    "constructs": "^3.2.47"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.75.0",
    "@aws-cdk/aws-lambda": "^1.75.0",
    "@aws-cdk/aws-s3": "^1.76.0",
    "@aws-cdk/core": "^1.75.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "aws-cdk": "^1.75.0",
    "aws-sdk": "^2.813.0",
    "conditional-type-checks": "^1.0.5",
    "esbuild": "^0.8.23",
    "husky": "^4.3.0",
    "javascript-stringify": "^2.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "cache": false,
    "globals": {
      "ts-jest": {
        "diagnostics": true,
        "tsconfig": "tsconfig.json"
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts",
      "**/tests/**/*"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "verbose": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run test --bail && npm run build"
    }
  },
  "scripts": {
    "clean": "rm -rf cdk.out cjs example/lambda/dist node_modules",
    "format": "prettier --write . --ignore=node_modules --loglevel=error",
    "test": "jest",
    "build": "tsc -P tsconfig.build.json",
    "release": "standard-version",
    "release-next": "npm run release -- --prerelease next",
    "example": "func() { cdk ${1} --app 'ts-node example';}; func",
    "deploy-example": "npm run example deploy",
    "destroy-example": "npm run example destroy"
  }
}
